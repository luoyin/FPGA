#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 7;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1842ba0 .scope module, "bench_top" "bench_top" 2 3;
 .timescale -6 -7;
v0x1851f10_0 .net "CLK", 0 0, v0x18436a0_0; 1 drivers
v0x1851fe0_0 .net "nRST", 0 0, v0x1851e70_0; 1 drivers
S_0x1842c90 .scope module, "uClock" "bench_clock" 2 7, 3 3, S_0x1842ba0;
 .timescale -6 -7;
v0x18436a0_0 .var "CLK_O", 0 0;
v0x1851e70_0 .var "nRST_O", 0 0;
    .scope S_0x1842c90;
T_0 ;
    %set/v v0x18436a0_0, 0, 1;
    %set/v v0x1851e70_0, 1, 1;
T_0.0 ;
    %delay 50, 0;
    %load/v 8, v0x18436a0_0, 1;
    %inv 8, 1;
    %set/v v0x18436a0_0, 8, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x1842c90;
T_1 ;
    %delay 20000, 0;
    %vpi_call 3 15 "$finish";
    %end;
    .thread T_1;
    .scope S_0x1842ba0;
T_2 ;
    %vpi_call 2 10 "$dumpvars", 1'sb0, S_0x1842c90;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "bench_top.v";
    "bench_clock.v";
